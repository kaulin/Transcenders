{
  "name": "transcenders",
  "private": true,
  "type": "module",
  "packageManager": "npm@11.5.2",
  "workspaces": [
    "infra/*",
    "packages/*",
    "services/*",
    "web"
  ],
  "description": "transcenders",
  "author": "",
  "license": "ISC",
  "scripts": {
    "with-env": "dotenv -e .env --",
    "dev": "turbo dev",
    "dev:packages": "turbo dev --filter=./packages/*",
    "dev:frontend": "turbo dev --filter=web",
    "dev:backend": "turbo dev --filter=./services/*",
    "dev:packsfront": "turbo dev --filter=./packages/* --filter=web",
    "dev:compiled": "turbo dev:compiled --filter=./services/*",
    "build": "turbo build",
    "build:packages": "turbo build --filter='./packages/*'",
    "build:frontend": "turbo build --filter='./packages/contracts' --filter='./packages/api-client' --filter=web",
    "build:backend": "turbo build --filter=./services/**",
    "build:watch": "turbo dev:compiled",
    "clean": "turbo clean && find . -name 'tsconfig.tsbuildinfo' -delete",
    "clean-turbo": "find . -name '.turbo' -print -exec rm -rf {} +",
    "start": "turbo start",
    "start:backend": "turbo start --filter='!web'",
    "test:env": "scripts/env-gen.js local && dotenv -e .env --",
    "test": "npm run test:env tsx scripts/tests.ts",
    "test:api": "npm run test:env tsx scripts/api-client-test.ts",
    "reglogin": "tsx scripts/reglogin.ts",
    "lint": "turbo lint",
    "lint:front": "turbo lint --filter=web",
    "lint:fix": "turbo lint:fix",
    "lint:check": "turbo lint --continue -- --max-warnings 0",
    "type-check": "turbo type-check",
    "type-check:watch": "tsc --project tsconfig.typecheck.json --watch",
    "gen:refs": "node scripts/gen-refs.js"
  },
  "devDependencies": {
    "clipboardy": "^4.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "turbo": "^2.5.6",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.39.1"
  }
}