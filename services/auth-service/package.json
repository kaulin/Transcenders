    "@transcenders/fastify-server": "*",
    "@transcenders/server-utils": "*",
    "bcrypt": "6.0.0",
    "fastify": "^5.5.0",
    "googleapis": "^156.0.0",
    "jsonwebtoken": "^9.0.2",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^5.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "tsx": "^4.20.4",
    "typescript": "~5.9.2"
  }
