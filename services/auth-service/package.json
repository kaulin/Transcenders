{
  "name": "auth-service",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --inspect=0.0.0.0:9228 src/server.ts",
    "dev:compiled": "node --watch --inspect=0.0.0.0:9228 dist/server.js",
    "debug": "tsx --inspect-brk=9228 src/server.ts",
